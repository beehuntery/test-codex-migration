{
  "name": "@test-codex/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:with-db": "prisma migrate deploy --schema ../../prisma/schema.prisma && prisma db push --schema ../../prisma/schema.prisma && next dev",
    "prebuild": "prisma generate --schema ../../prisma/schema.prisma",
    "build": "prisma migrate deploy --schema ../../prisma/schema.prisma && prisma db push --schema ../../prisma/schema.prisma && next build",
    "start": "prisma db push --schema ../../prisma/schema.prisma && next start",
    "lint": "eslint --max-warnings=0 .",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.0-alpha.14"
  },
  "devDependencies": {
    "prisma": "^5.22.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/postcss": "^4.0.0-alpha.14",
    "@types/node": "^24.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.1",
    "postcss": "^8.4.47",
    "storybook": "^10.0.8",
    "typescript": "^5.6.3"
  },
  "postcss": {
    "plugins": {
      "@tailwindcss/postcss": {}
    }
  }
}
